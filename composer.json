{
    "name": "efabrica/phpstan-rules",
    "license": ["MIT"],
    "require": {
        "php": ">=7.4 <8.5",
        "ext-json": "*",
        "phpstan/phpstan": "^1.8"
    },
    "require-dev": {
        "nikic/php-parser": "^4.14",
        "tracy/tracy": "*",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3",
        "tomaj/nette-api": "^3.0.0",
        "efabrica/php-extensions-finder": "^0.5"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
