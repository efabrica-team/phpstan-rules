{
    "name": "efabrica/phpstan-rules",
    "license": ["MIT"],
    "require": {
        "php": ">=8.0 <8.2",
        "phpstan/phpstan": "^1.6",
        "symplify/astral": "^10.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
