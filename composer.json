{
    "name": "efabrica/phpstan-rules",
    "license": ["MIT"],
    "require": {
        "php": ">=7.4 <=8.2",
        "nikic/php-parser": "^4.14",
        "phpstan/phpstan": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3",
        "tomaj/nette-api": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Efabrica\\PHPStanRules\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
